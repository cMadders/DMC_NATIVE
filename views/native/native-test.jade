doctype html
html
  head
    title #{title}
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
    meta(name='viewport', content='width=device-width, initial-scale=1, maximum-scale=1')
    link(rel='stylesheet', href='http://netdna.bootstrapcdn.com/bootstrap/3.0.3/css/bootstrap.min.css')
    style.
      body{
        background-color:#CCC;
      }

      .dmc-native{
        box-sizing:border-box;
      }

    //- card list css
    link(rel='stylesheet' href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css")
    link(rel='stylesheet' href="https://fonts.googleapis.com/css?family=Raleway:400,700")
    style.
      img{
        vertical-align:inherit;
      }
      #dmc-card-container {
        width: 375px;
        margin: 0 auto;
        background-color: #FFF;
        font-family: 'Raleway', sans-serif;
        font-weight: 400;
      }
      #dmc-card-container h1,
      #dmc-card-container h2,
      #dmc-card-container h3,
      #dmc-card-container p,
      #dmc-card-container li,
      #dmc-card-container a,
      #dmc-card-container button {
        font-family: 'Raleway', sans-serif;
        font-weight: 400;
      }
      #dmc-card-container strong,
      #dmc-card-container b {
        font-weigth: 700;
      }
      #dmc-card-container .card-list {
        position: relative;
        overflow: hidden;
        height: 627px;
      }
      #dmc-card-container .card-list .card-header {
        flex-direction: row;
        display: flex;
        justify-content: space-between;
        align-items: center;
        border-bottom: 1px solid #AAA;
      }
      #dmc-card-container .card-list .card-header .back {
        display: none;
      }
      #dmc-card-container .card-list .card-header .filter,
      #dmc-card-container .card-list .card-header .closeit,
      #dmc-card-container .card-list .card-header .back {
        width: 55px;
        height: 55px;
        text-align: center;
      }
      #dmc-card-container .card-list .card-header .filter i,
      #dmc-card-container .card-list .card-header .closeit i,
      #dmc-card-container .card-list .card-header .back i {
        font-size: 36px;
        padding: 8px 0;
      }
      #dmc-card-container .card-list .card-header .filter {
        border-right: 1px solid #AAA;
      }
      #dmc-card-container .card-list .card-header .filter i {
        padding-top: 10px;
      }
      #dmc-card-container .card-list .card-header .closeit {
        border-left: 1px solid #AAA;
      }
      #dmc-card-container .card-list .card-header .logo {
        width: calc(100% - 130px);
        overflow: hidden;
      }
      #dmc-card-container .card-list .card-header .logo img {
        width: 100%;
      }
      #dmc-card-container .card-list .card-body .item {
        cursor: pointer;
        padding: 8px;
        border-bottom: 1px solid #eee;
        overflow: hidden;
      }
      #dmc-card-container .card-list .card-body .item .thumbnail {
        width: 120px;
        height: 48px;
        background-size: cover;
        background-position: 50%;
        background-repeat: no-repeat;
        background-color: #eee;
        float: left;
        padding: 0;
        margin: 0;
        line-height: inherit;
        border: none;
        border-radius: inherit;
        margin-right: 8px;
      }
      #dmc-card-container .card-list .card-body .item .text {
        display: flex;
        flex-direction: column;
        justify-content: center;
        height: 48px;
      }
      #dmc-card-container .card-list .card-body .item .text .title {
        font-weight: 700;
        font-size: 15px;
      }
      #dmc-card-container .card-list .card-body .item .text .description {
        font-size: 11px;
      }
      #dmc-card-container .card-list .card-footer {
        position: absolute;
        padding: 6px;
        bottom: 0;
        width: 100%;
        flex-direction: row;
        display: flex;
        justify-content: space-between;
        align-items: center;
        box-sizing: border-box;
        background-color: #ccc;
        border-top: 1px solid #AAA;
      }
      #dmc-card-container .card-list .card-footer button {
        border-style: solid;
        color: #FFF;
        font-size: 16px;
        height: 30px;
      }
      #dmc-card-container .card-list .card-footer button.apply {
        background-color: #FF3B30;
        border-color: #FF3B30;
        padding: 0 25px;
      }
      #dmc-card-container .card-list .card-footer button.apply p {
        margin: 0;
        padding: 0;
        text-shadow: rgba(0, 0, 0, 0.8) 1px 1px 1px;
      }
      #dmc-card-container .card-list .card-footer .share button {
        width: 40px;
      }
      #dmc-card-container .card-list .card-footer .share button i {
        text-shadow: rgba(0, 0, 0, 0.8) 1px 1px 1px;
      }
      #dmc-card-container .card-list .card-footer .share button.fb {
        background-color: #2c4270;
        border-color: #2c4270;
      }
      #dmc-card-container .card-list .card-footer .share button.twitter {
        background-color: #1B9AF7;
        border-color: #1B9AF7;
      }
      #dmc-card-container .card-list .card-footer .share button.sms {
        background-color: #0BD318;
        border-color: #0BD318;
      }

    //- temporary scripts to speed build process
    script(type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.2.0/jquery.min.js")
    //- script(type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/zepto/1.1.6/zepto.min.js")
    script(type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.8.3/underscore-min.js")
    script(type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/gsap/1.18.2/plugins/CSSPlugin.min.js")
    script(type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/gsap/1.18.2/easing/EasePack.min.js")
    script(type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/gsap/1.18.2/TweenLite.min.js")

